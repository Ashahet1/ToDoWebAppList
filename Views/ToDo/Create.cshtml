@model ToDoWebAppList.Models.TodoItem

<h1>Add New Todo Item</h1>

<form asp-action="Create" method="post">
    <div>
        <label>Title:</label>
        <input asp-for="Title" />
    </div>
    <div>
        <label>Is Complete:</label>
        <input asp-for="IsComplete" type="checkbox" />
    </div>
    <button type="submit">Save</button>
</form>
